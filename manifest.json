{
    "name": "Svenskafans textruta",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Ändra storlek på textrutan i 'Meddelande' samt 'Forum' till valfri storlek",
    "browser_action": {
        "default_title": "Svenskafans textruta"
    },
    "icons": {
        "16": "/images/logo16.png",
        "48": "/images/logo48.png",
        "128": "/images/logo128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.svenskafans.com/*"
            ],
            "js": [
                "/styling/styles.js"
            ]
        }
    ]
}